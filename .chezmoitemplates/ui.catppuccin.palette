{{- $theme := "" -}}
{{- if eq .ui.theme.choice "auto" -}}
{{-     $theme = output "sh" (joinPath .chezmoi.sourceDir "_helpers/get-os-theme.sh") | trim -}}
{{- else -}}
{{-     $theme = .ui.theme.choice -}}
{{- end -}}

{{- $palettes := dict "light" "latte" "dark" "macchiato" -}}
{{- get $palettes $theme -}}
